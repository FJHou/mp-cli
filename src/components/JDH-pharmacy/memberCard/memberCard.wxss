.name-group {
    line-height: 40rpx;
    display: flex;
    align-items: center;
}
.name {
    font-size: 30rpx;
    font-weight: bold;
    background: #fff;
    background-clip: text;
    -webkit-background-clip: text;
    color: transparent;
}
.divider {
    display: inline-block;
    height: 14rpx;
    width: 1rpx;
    margin: 0 8rpx;
}
.number {
    font-size: 24rpx;
    margin-left: 14rpx;
    background: var(--color-platinum);
    background-clip: text;
    -webkit-background-clip: text;
    color: transparent;
}

.logined-score {
    display: inline-flex;
    align-items: flex-end;
}

.icon-crown {
    width: 40rpx;
    height: 34rpx;
    background: center/100% no-repeat;
}

.icon-level {
    width: 30rpx;
    height: 20rpx;
    vertical-align: middle;
    background-image: var(--crown-level-normal);
    border-radius: 2rpx;
    margin-right: 20rpx;
    background: center/100% no-repeat;
}
.score {
    font-size: 22rpx;
    line-height: 22rpx;
    background: #fff;
    background-clip: text;
    -webkit-background-clip: text;
    color: transparent;
}

.card-button {
    width: 124rpx;
    height: 48rpx;
    font-size: 24rpx;
    line-height: 48rpx;
    border-radius: 32rpx;
    text-align: center;
    background-image: url(https://img10.360buyimg.com/imagetools/jfs/t1/159518/5/8187/121229/603496e4Eaccadf78/e4daf75a562a0b0a.gif);
    background-size: 100%;
    background-position: center;
    color: var(--button-color-platinum);
    background-color: linear-gradient(135deg, rgba(242,20,12,1) 0%,rgba(242,77,12,1) 100%);
    font-weight: bold;
}

.score-desc {
    display: flex;
    align-items: center;
    font-size: 22rpx;
}

/* 普通会员 */
