/* pages/myCoupon/myCoupon.wxss */
@import '../../css/JDfont.wxss';

.coupon-title {
	box-sizing: border-box;
	width: 100%;
	height: 150rpx;
	padding: 30rpx 24rpx;
	margin-bottom: 20rpx;
	background: #fff;
}
.shop-Img {
	float: left;
	width: 90rpx;
	height: 90rpx;
}
.coupon-shop {
	position: relative;
	margin-left: 114rpx;
	margin-right: 140rpx;
}
.shop-name {
	line-height: 36rpx;
	font-family: PingFangSC-Semibold;
	font-size: 36rpx;
	color: rgba(38, 38, 38, 1);
	margin-bottom: 20rpx;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.shop-address {
	line-height: 24rpx;
	font-family: PingFangSC-Regular;
	font-size: 24rpx;
	color: rgba(38, 38, 38, 1);
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}
.coupon-goToShop {
	position: absolute;
	top: 48rpx;
	right: 24rpx;
	width: 124rpx;
	height: 54rpx;
	text-align: center;
	background: linear-gradient(315deg, rgba(255, 8, 0, 1) 0%, rgba(255, 103, 44, 1) 100%);
	border-radius: 26rpx;
	line-height: 54rpx;
	font-family: PingFangSC-Medium;
	font-size: 22rpx;
	color: rgba(255, 255, 255, 1);
}

.fid-btn {
	padding: 0;
	margin: 0;
	background-color: transparent;
	border: 0 none;
	-webkit-tap-highlight: none;
	line-height: 48rpx;
	font-size: 22rpx;
}
.white {
	color: #fff;
}
page {
	background: rgb(248, 248, 248);
	height: 100%;
	font-family: PingFang-SC-Regular;
}
.swiper-tab {
	text-align: center;
	display: flex;
	height: 90rpx;
	justify-content: center;
	align-items: center;
	border-top: 1px solid rgb(244, 244, 244);
	background: white;
	position: fixed;
	top: 170rpx;
	left: 0;
	right: 0;
	z-index: 999;
}
.blank {
	height: 90rpx;
}
.swiper-tab-list {
	position: relative;
	line-height: 90rpx;
	font-family: PingFangSC-Regular;
	font-size: 28rpx;
	color: rgba(38, 38, 38, 1);
	flex: 1;
}
.swiper-tab-list .btline {
	position: absolute;
	bottom: 0;
	width: 50rpx;
	margin-left: -25rpx;
	left: 50%;
	height: 10rpx;
	display: block;
	background: linear-gradient(90deg, rgba(250, 87, 55, 1) 0%, rgba(252, 215, 207, 0.5) 100%);
	border-radius: 6rpx;
}
.btline.nods {
	left: 47%;
}
.on {
	font-family: PingFangSC-Semibold;
	font-weight: 600;
	font-size: 28rpx;
	color: rgba(38, 38, 38, 1);
}
.swiper-tab-list.on .btline {
	margin-left: -24rpx;
}
.swiper-box {
	display: block;
	width: 100%;
	overflow: hidden;
	height: 100%;
	background-color: #fff;
}
.bt-bottom {
	position: relative;
	padding-bottom: calc(env(safe-area-inset-bottom) + 20rpx);
}
.bt-blank {
	height: 120rpx;
}
.bt {
	position: absolute;
	bottom: 36rpx;
	left: 50%;
	margin-left: -50rpx;
}
.bt-na {
	color: rgb(196, 196, 196);
	font-size: 20rpx;
	font-family: PingFangSC-Regular;
	line-height: 28rpx;
	margin: 0rpx 0 6rpx;
	text-align: center;
}
.logo {
	margin: 0 auto;
	display: block;
	width: 43rpx;
	height: 37rpx;
}

.coupon-view {
	padding: 20rpx 0 0;
	/* box-sizing: border-box; */
}
.coupon-view-aluse {
	padding-top: 23rpx;
}
.coupon-fsuse {
	margin-top: 16rpx;
	font-size: 30rpx;
	line-height: 60rpx;
	width: 180rpx;
	border: 2rpx solid #ff7873;
	color: #ff7873;
	border-radius: 60rpx;
	text-align: center;
}

.coupon-aluse {
	height: 328rpx;
	background: white;
	border-radius: 16rpx;
	margin: 0 10rpx;
	position: relative;
	overflow: hidden;
}
.coupon-aluse-top {
	height: 178rpx;
	background: rgb(194, 194, 194);
}
.coupon-aluse-mid {
	position: absolute;
	width: 100%;
	left: 0;
	top: 154rpx;
	height: 48rpx;
}
.coupon-mid-box {
	position: relative;
	padding: 0 32rpx;
	display: flex;
	height: 48rpx;
	justify-content: space-around;
	align-items: center;
}
.coupon-mid-box view {
	width: 12rpx;
	height: 12rpx;
	background: #fff;
	margin: 0 6rpx;
	border-radius: 50%;
}
.coupon-mid-box view.left-half {
	width: 48rpx;
	height: 48rpx;
	border-radius: 50%;
	background: rgb(248, 248, 248);
	position: absolute;
	left: -24rpx;
	top: 0;
	margin: 0;
}
.coupon-mid-box view.right-half {
	width: 48rpx;
	height: 48rpx;
	border-radius: 50%;
	background: rgb(248, 248, 248);
	position: absolute;
	right: -24rpx;
	top: 0;
	margin: 0;
}

.coupon-aluse-sign {
	width: 152rpx;
	height: 152rpx;
	position: absolute;
	top: 0;
	right: 30rpx;
}
.none-coupon {
	display: flex;
	flex-direction: column;
	justify-content: space-around;
	align-items: center;
	padding-top: 132rpx;
}
.none-coupon image {
	width: 260rpx;
	height: 260rpx;
	margin-bottom: 57rpx;
	display: block;
}
.none-coupon-word {
	color: rgb(132, 132, 132);
	line-height: 37rpx;
	font-size: 26rpx;
	margin-bottom: 30rpx;
}

.f8be360ead1f4a3d9afb5e312b845ebb {
	position: absolute;
	width: 100%;
	height: 100%;
	border: solid 1px #cdcdcd;
	opacity: 0;
	left: -100%;
	top: -100%;
	z-index: -2147483648;
}
