/* 图片上传的样式 */
.upload-image-item-big {
  width: 330rpx;
  height: 176rpx;
  position: relative;
}

.upload-image-icon-big {
  display: inline-block;
  font-size: 72rpx;
  margin: 30rpx 0rpx 18rpx 0rpx;
}

.upload-image-item-pic-big {
  width: 330rpx;
  height: 176rpx;
}

.upload-image-wrap{
  display: flex;
  flex-wrap: wrap;
  align-items: center;
  justify-content: flex-start;
  margin-left:30rpx;
}
.upload-image-item{
  width: 160rpx;
  height: 156rpx;
  border: 1rpx solid #D8DCE0;
  margin-right: 35rpx;
  position: relative;
  border-radius: 8rpx;
}
.upload-image-btn{
  background: #FFFFFF;
  border: 1rpx solid #D8DCE0;
  color: #D8DCE0;
  text-align: center;
}
.upload-image-btn::after{
  border: 0rpx;
}
.upload-image-icon{
  display: inline-block;
  font-size: 36rpx;
  margin: 36rpx 0rpx 18rpx 0rpx;
}
.upload-image-text{
  font-size: 24rpx;
  color: #D8DCE0;
  line-height: 24rpx;
}
.upload-image-item-pic{
  width: 160rpx;
  height: 160rpx;
}
.upload-image-remove-btn{
  position: absolute;
  right: -20rpx;
  top: -20rpx;
  font-size: 40rpx;
  color: #666;
  display: inline-block;
  height: 40rpx;
  width: 40rpx;
  background-color:#fff;
  border-radius: 100%;
}

