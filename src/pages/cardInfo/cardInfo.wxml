<!-- pages/cardInfo/cardInfo.wxml -->
<view class='content card-inof-content'>
  <view class='p_top' style="background:#ccc url(https://thunder.jd.com/jpass/img/card_bg2.png);background-size:100% 100%;">
    <view class="cardli" bindtap="checkMembersPoint" style="background:{{liberty.cardColor ? liberty.cardColor : '#ccc'}} url({{liberty.cardUrl ? liberty.cardUrl : 'https://thunder.jd.com/jpass/img/card_deco.png'}}); background-size:100% 100%;">
      <view class="card-h">
        <image class="logo" mode='widthFix' src='{{liberty.brandLogo}}'></image>
        <text class="name textellipse">{{liberty.brandName}}</text>
        <view class="grade" wx:if="{{liberty.points != null}}">
          <text class="gredenum">{{liberty.points}}</text>
          积分 >
        </view>
      </view>
      <view class="card-b">
        <view class="num">
          <text wx:if="{{liberty.cardNo}}">NO. {{liberty.cardNo}}</text>
        </view>
        <text class="ctype">{{liberty.customerLevelName}}</text>
      </view>
    </view>
  </view>
  <view class='zhan'></view>
  <view class='liberty now-right'>
    <view class='liberty-tit'>
      <image src='https://thunder.jd.com/jpass/img/j4icon_liberty@3x.png'></image>
      <text>当前等级特权</text>
    </view>
    <view class='liberty-content'>
      <view class='liberty-item' wx:for="{{liberty.rightsList}}" wx:key="index" data-url="{{item.url}}" bindtap="checkLevelRight">
        <image class='logo' src="{{item.icon}}"></image>
        <text>{{item.name}}</text>
      </view>
    </view>
  </view>
  <view wx:if="{{showliber}}" class='liberty'>
    <view class='liberty-tit'>
      <image src='https://thunder.jd.com/jpass/img/level_next.png'></image>
      <text class="right-text">下一等级特权</text>
      <!-- <text class="rule-text" bindtap="openRulePro">升级规则</text>
      <image class="icon-more" src='https://thunder.jd.com/jpass/img/icon_more.png' bindtap="openRulePro"></image> -->
    </view>
    <view class='liberty-content'>
      <view class='liberty-item' wx:for="{{liberty.nextRightsList}}" wx:key="index" data-url="{{item.url}}" bindtap="checkLevelRight">
        <image class='logo' src="{{item.icon}}"></image>
        <text>{{item.name}}</text>
      </view>
    </view>
  </view>
  <view bindtap='toggle' wx:if="{{!showliber && liberty.nextRightsList.length > 0}}" class='liberbtn'>
    查看下一级等级特权
    <image src='https://thunder.jd.com/jpass/img/more_open.png'></image>
  </view>
  <view bindtap='toggle' wx:if="{{showliber}}" class='liberbtn'>
    收起下一级等级特权
    <image src='https://thunder.jd.com/jpass/img/more_close.png'></image>
  </view>
  <view class='member' wx:if="{{hasGift}}">
    <view class='member-tit'>
      <image src='https://thunder.jd.com/jpass/img/gift.png'></image>
      <text>会员专属活动</text>
    </view>
    <view class='member-gift'>
      <image src='https://thunder.jd.com/jpass/img/giftbag_member_day.jpeg'></image>
      <view class='member-gtmsg'>
        <view class='member-gtmsg-tit'>会员日礼包</view>
        <view class='member-gtmsg-sub'>为您精心准备，购物更优惠</view>
      </view>
      <view class='member-btn' data-eid="card_information_memberday" bindtap='openGift' data-type="hasGiftStatus">
        立即领取
      </view>
    </view>
  </view>
