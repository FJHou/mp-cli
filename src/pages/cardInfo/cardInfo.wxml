<image bindtap='openGift' data-type="memHasGiftStatus" data-eid="card_information_couponpack" wx:if="{{memHasGift}}" class='gift-float' mode='widthFix' src='https://thunder.jd.com/jpass/img/gift_float.png'></image>
<giftbag wx:if="{{hasGift && hasGiftStatus}}" brandid="{{param.brandId}}" bid="{{param.bId}}" brandsid="{{param.brandsId}}" fromtype="{{fromtype}}" list="{{couponList}}" activityid="{{activityid}}" activitytype="{{activitytype}}" bind:closegift="closegift"></giftbag>
<giftbag wx:if="{{memHasGift && memHasGiftStatus}}" brandid="{{param.brandId}}" bid="{{param.bId}}" brandsid="{{param.brandsId}}" fromtype="{{fromtype}}" list="{{memCouponList}}" activityid="{{memActivityid}}" activitytype="{{memActivitytype}}" bind:closegift="closegift"></giftbag>
