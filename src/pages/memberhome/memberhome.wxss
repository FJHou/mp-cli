/* 用户信息 */
.unmember-wraper .user-info{
  height:180rpx;
  position:relative;
  overflow:hidden;
}
.unmember-wraper .user-info .user-info-username{
  color: #333333;
  letter-spacing: 0;
  margin-top:90rpx;
  margin-left:30rpx;
  width:308rpx;
  /* height:54rpx; */
  word-break: break-all;
  -webkit-line-clamp: 1; /* 限制在一个块元素显示的文本的行数 */
  line-clamp: 1;
  display: -webkit-box; /* 将对象作为弹性伸缩盒子模型显示 */
  -webkit-box-orient: vertical; /* 设置或检索伸缩盒对象的子元素的排列方式 */
  overflow: hidden;
}
.unmember-wraper .user-info .user-info-img{
  width:341rpx;
  height:343rpx;
  position:absolute;
  right:-100rpx;
  top:-160rpx;
}
.unmember-wraper .user-info .user-info-img .user-info-img-bg-dark{
  border:70rpx solid #FC1902;
  border-radius: 50%;
  width:190rpx;
  height:200rpx;
  position:absolute;
  z-index:3;
}
.unmember-wraper .user-info .user-info-img .user-info-img-bg-light{
  border:70rpx solid #fff8f0;
  border-radius: 50%;
  width:190rpx;
  height:200rpx;
  position:absolute;
  z-index:1;
  left:60rpx;
}
.unmember-wraper .user-info .user-info-img .user-info-img-head{
  width:110rpx;
  height:110rpx;
  border:7rpx solid #fff;
  border-radius: 50%;
  box-shadow: 0 0 20px 0 rgba(0,0,0,0.10);
  position:absolute;
  bottom:10rpx;
  right:134rpx;
  background:#eee;
  z-index:5;
}
.unmember-wraper .user-info .user-info-img .user-info-img-head image{
  width:100%;
  height:100%;
