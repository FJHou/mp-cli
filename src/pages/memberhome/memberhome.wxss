/* pages/memberhome/memberhome.wxss */
@import './forecastEarnings/forecastEarnings.wxss';


/* 我的专属老师 */
.my-teacher{
  position: absolute;
  right: 0;
  height: 56rpx;
  width: 290rpx;
  background: #FBEAE8;
  border-radius: 28rpx 0 0 28rpx;
  top:10rpx;
  display: flex;
  line-height: 56rpx;
  justify-content:space-around;
  padding: 0 10rpx 0 30rpx;
  box-sizing: border-box;
  z-index: 1000;
}
.my-teacher .icon{
  background: linear-gradient(0deg, #EA4364 0%, #F97B58 100%);
  -webkit-background-clip: text;
  color: transparent;
  -webkit-text-fill-color: transparent;
  text-fill-color: transparent;
}
/* 我的老师弹窗 */
.mask-box{
  position: fixed;
  top: 0;
  width: 100%;
  height: 100%;
  background: rgba(0, 0, 0, 0.5);
  z-index: 1990;
}
.tanceng-box{
  width: 550rpx;
  height: 470rpx;
  background: #fff;
  border-radius: 20rpx;
  margin: 320rpx auto 0;
  text-align: center;
  position: relative;
  padding-top: 30rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.no-teacher{
  width: 550rpx;
  height: 316rpx;
  background: #fff;
  border-radius: 20rpx;
  margin: 220rpx auto 0;
  text-align: center;
  position: relative;
  padding-top: 30rpx;
  box-sizing: border-box;
  overflow: hidden;
}
.warning-note{
  background: #f7f7f7;
  width: 490rpx;
  height: 160rpx;
  margin: 30rpx auto;
  border-radius: 8rpx;
  padding: 20rpx;
  box-sizing: border-box;
  line-height: 40rpx;
}
.tanceng-btn{
  position: absolute;
  bottom: 0;
  border-top: 2rpx solid #E5E6E8;
  height: 88rpx;
  line-height: 88rpx;
  width: 100%;
}
.tanceng-btn text{
  width: 50%;
  text-align: center;
  display: inline-block;
  border-right: 2rpx solid #E5E6E8;
  box-sizing: border-box;
}



.modals-cancel {
  position: fixed;
  z-index: 1000;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.8);
  width: 750rpx;
}
.longName{
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 1;
  line-clamp: 1;
  -webkit-box-orient: vertical;
}

