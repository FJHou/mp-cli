.income-wrap{
  background: #FFFFFF;
  box-shadow: 0 0 40px 0 rgba(0,0,0,0.10);
  border-radius: 12px;
  margin:30rpx 30rpx 0;
  padding:20rpx;
  font-size:30rpx;
  color:#333;
}
.income-wrap-title{
  padding-bottom:20rpx;
  font-size:34rpx;
}
.income-wrap-cont{
  padding:20rpx 0;
  color:#999;
}
.income-wrap-sub{
  padding:10rpx 0;
  font-size:24rpx;
  color:#bbb;
}
.income-wrap-title~.income-wrap-cont{
  border-top:2rpx solid #f2f2f2;
}





