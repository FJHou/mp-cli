<page class="wrap get-coupon">
  <!-- 可用优惠券 -->
        <!-- 东券 -->
        <view wx:if="{{item.couponType == 1}}" class="coupon-box blue">
          <view class="left-box">
            <view class="m-box">
              <view class="f30">¥</view>
              <view class="f76">{{item.discount}}</view>
            </view>
            <view class="n-m">满{{item.quota}}元可用</view>
            <view class="r"></view>
          </view>
          <view class="right-box">
            <view class="right-l">
              <view class="coupon-l">
                <view class="coupon-bg">东券</view>
                <view class="coupon-font">{{item.couponTypeMsg?item.couponTypeMsg:''}}</view>
              </view>
              <view class='overlay-wrap'>
              </view>
              <view class="use-t">{{item.couponDateRange}}</view>
            </view>
            <!-- 领取标识戳 begin -->
            <view class="icn-coupon-status {{couponStatusCls}}"></view>
            <!-- 领取标识戳 end -->
          </view>
        </view>
        <!-- 京券 -->
      <view wx:elif="{{item.couponType == 0}}" class="coupon-box red">
        <view class="left-box">
          <view class="m-box">
            <view class="f30">¥</view>
            <view class="f76">{{item.discount}}</view>
          </view>
          <view class="n-m">满{{item.quota}}元可用</view>
          <view class="r"></view>
        </view>
        <view class="right-box">
          <view class="right-l">
            <view class="coupon-l">
              <view class="coupon-bg">京券</view>
                <view class="coupon-font">{{item.couponTypeMsg?item.couponTypeMsg:''}}</view>
            </view>
            <view class='overlay-wrap'></view>
            <view class="use-t">{{item.couponDateRange}}</view>
          </view>
          <!-- 领取标识戳 begin -->
            <view class="icn-coupon-status {{couponStatusCls}}"></view>
            <!-- 领取标识戳 end -->
        </view>
      </view>
      <!-- 运费券 -->
        <view wx:elif="{{item.couponType == 2}}" class="coupon-box green">
          <view class="left-box">
            <view class="m-box">
              <view class="f36">可抵扣{{item.discount}}元</view>
            </view>
            <!--<view class="n-m">除偏远山区、极速达</view>-->
            <view class="r"></view>
          </view>
          <view class="right-box">
            <view class="right-l">
              <view class="coupon-l">
                <view class="coupon-bg">运费券</view>
                <view class="coupon-font">{{item.couponTypeMsg?item.couponTypeMsg:''}}</view>
              </view>
              <view class='overlay-wrap'></view>
              <view class="use-t">{{item.couponDateRange}}</view>
            </view>
            <view class="right-r">
              <view class="coupon-tip" bindtap="couponShowInfo">
                <view class="info-icon"></view>
              </view>
            </view>
            <!-- 领取标识戳 begin -->
            <view class="icn-coupon-status {{couponStatusCls}}"></view>
            <!-- 领取标识戳 end -->
          </view>
        </view>
    <block wx:if="{{isLogined==true}}">
    <block wx:if="{{ tabFun !='tapBtnBack' }}">
        <form report-submit="true" bindsubmit="{{tabFun}}">
            <button class="button {{couponDisabled}} message-push-btn" form-type="submit">{{buttonTxt}}</button>
        </form>
        <!-- <view class="button {{couponDisabled}}" bindtap='{{tabFun}}'>{{buttonTxt}}</view> -->
    </block>
    <block wx:else>
        <form report-submit="true" bindsubmit="{{tabFun}}" wx:if="{{ !isJumpOut }}">
          <button class="button {{couponDisabled}} message-push-btn" form-type="submit">{{buttonTxt}}</button>
        </form>
        <!-- <view wx:if="{{ !isJumpOut }}" class="button {{couponDisabled}}" bindtap='{{tabFun}}'>{{buttonTxt}}</view> -->
        <navigator wx:else class="button {{couponDisabled}}" target="miniProgram" app-id="{{ miniApp.appId }}" version="{{ miniApp.version }}" path="{{ miniApp.path }}">{{buttonTxt}}</navigator>
    </block>    
    <view class="coupon-tip-msg">{{couponTipMsg}}</view>
  </block>
  
</page>